{
  "version": 2,
  "builds": [
    { 
      "src": "server.js", 
      "use": "@vercel/node" 
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": "node_modules/@ffprobe-installer/linux-x64/ffprobe"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}